		self.fig.clear()
		plt.style.use(MATE.plot_style_list[MATE.plot_style_selection])
		self.canvas.draw()

		self.drawn = 0
		self.drawn_1 = 0
		self.drawn_11 = 0
		self.drawn_3 = 0
		self.drawn_4 = 0
		self.drawn_5 = 0

		self.T_list = []
		self.bulk_cond_max_list = []
		self.bulk_cond_min_list = []
		self.bulk_cond_list = []
		self.linestyle_list = []
		self.linewidth_list = []
		self.h2o_bulk_list = []
		self.h2o_list = []
